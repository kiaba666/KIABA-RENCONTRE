[flake8]
max-line-length = 140
extend-ignore = E203, W503
exclude = .venv,build,dist,migrations,management/commands
per-file-ignores =
    ads/admin.py:F401
    core/admin.py:F401
    moderation/admin.py:F401
    seo/admin.py:F401
    moderation/models.py:F401
    seo/models.py:F401
    moderation/tests.py:F401
    seo/tests.py:F401
    seo/views.py:F401
    core/context_processors.py:W391
    core/views.py:F401
